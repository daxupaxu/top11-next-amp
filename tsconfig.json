{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "module": "esnext",
      "esModuleInterop": true,
      "target": "esnext",
      "lib": [
        "esnext"
      ],
      "jsx": "preserve",
      "downlevelIteration": true,
      "baseUrl": ".",
      "allowJs": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "paths": {
      "*": [
        "node_modules/*"
      ]
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx", "pages/_document.js", "utils/contentfulTopList.js"
    ]
  }